{
  "name": "ts-mastery",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "A collection of TypeScript Node.js katas and hands-on exercises covering Node runtime topics from beginner to advanced (buffers, events, streams, worker threads, and more). Designed for learning and exploration using Node.js v24 and TypeScript.",
  "main": "src/cli/index.tsx",
  "scripts": {
    "cli": "tsx src/cli/index.tsx",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "katas",
    "exercises",
    "node",
    "learning",
    "hands-on",
    "node-katas",
    "typescript-exercises",
    "ink-cli",
    "tsx",
    "beginner",
    "intermediate",
    "advanced"
  ],
  "author": "sattwyk",
  "license": "MIT",
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "dependencies": {
    "ink": "^6.4.0",
    "react": "^19.2.0"
  }
}
